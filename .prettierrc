{
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
        "^(react/(.*)$)|^(react$)",
        "^(@inertiajs/(.*)$)|^(@inertiajs$)",
        "<THIRD_PARTY_MODULES>",
        "",
        "^types$",
        "^~/types",
        "^~/types/(.*)$",
        "^~/config/(.*)$",
        "^~/lib/(.*)$",
        "^~/hooks/(.*)$",
        "^~/components/ui/(.*)$",
        "^~/components/(.*)$",
        "^~/Components/(.*)$",
        "",
        "^[./]"
    ]
}
